server {
        server_name {{ stage }}.{{ host_name }};
        listen 80;
        listen [::]:80;
        
        location / {
                try_files $uri $uri/ /index.html;
                root {{ www_stage_root_latest }};
        }

        access_log {{ logs_stage_root }}/access;
        error_log {{ logs_stage_root }}/error;
}